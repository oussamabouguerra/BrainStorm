                                                                }, success: function (r) {
                                                                    uID = r.id;
                                                                    localStorage.setItem('uID', uID);
                                                                }
                                                            });
                                                        }

                                                        function setStars(max) {
                                                            for (var i=0; i <= max; i++)
                                                                $('.fa-star:eq('+i+')').css('color', 'yellow');
                                                        }

                                                        function resetStarColors() {
                                                            $('.fa-star').css('color', 'black');
                                                        }
