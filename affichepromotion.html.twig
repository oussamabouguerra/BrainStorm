
{% extends 'baseFront.html.twig' %}

{% block body %}

    <section class="dark-wrapper opaqued parallax" data-parallax="scroll" data-image-src="assets/img/bg/bg2.jpg" data-speed="0.7">
        <div class="section-inner pad-top-200">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 mt30 wow text-center">
                        <h2 class="section-heading">Promotions</h2>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section>
        <table class="table m-table m-table--head-bg-success">
            <thead>
            <tr>
                <th>
                    id
                </th>
                <th>
                    nom
                </th>

                <th>
                    type
                </th>

                <th>
                    pourcentage
                </th>

                <th>
                    description
                </th>
                <th>
                    categorie
                </th>
                <th>
                    code
                </th>
                <th>
                    Modifier

                </th>
                <th>
                    Delete

                </th>



            </tr>
            </thead>

            {% for U in promotion%}
            <tr>
                <td>

                    {{ U.id }}
                </td>
                <td>
                    {{ U.nom }}
                </td>

                <td>
                    {{ U.type }}
                </td>
                <td>
                    {{ U.pourcentage }}
                </td>
                <td>
                    {{ U.description }}
                </td>
                <td>
                    {{ U.cat.categorie }}
                </td>
                <td>
                    {{ U.code }}
                </td>
                {% endfor %}
        </table>
    </section>
{% endblock %}
